{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "@zoychat/app",
	"version": "0.0.1-dev",
	"private": true,
	"scripts": {
		"expo:prebuild": "expo prebuild --clean",
		"expo:android": "expo start --android",
		"expo:ios": "expo start --ios",
		"expo:web": "expo start --web",
		"build:ios": "eas build -p ios",
		"build:android": "eas build -p android",
		"build:web": "expo export -p web",
		"build:desktop": "tauri build",
		"expo:start": "expo start",
		"lint": "prettier --check . && eslint src --ext .mjs,.js,.cjs,.ts,.tsx --format=pretty",
		"format": "prettier --write . && eslint src --ext .mjs,.js,.cjs,.ts,.tsx --fix --format=pretty",
		"fmt": "yarn format"
	},
	"main": "expo-router/entry",
	"contributors": [
		"Nicolas Ribeiro <git@niskii.dev>"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/zoychat/app"
	},
	"bugs": {
		"url": "https://github.com/zoychat/app/issues"
	},
	"homepage": "",
